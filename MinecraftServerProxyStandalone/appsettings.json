{
  "ProxyConfiguration": {
    "IPAddress": "0.0.0.0",
    "Port": 25565,
    "Servers": {
      "connor1.localhost": {
        "IPAddress": "127.0.0.1",
        "Port": 25570
      },
      "connor2.localhost": {
        "IPAddress": "127.0.0.1",
        "Port": 25571
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "fileSizeLimitBytes": 20971520,
          "rollOnFileSizeLimit": true,
          "retainedFileCountLimit": 3
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
